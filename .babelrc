{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": ["@babel/transform-runtime",
        ["@babel/plugin-proposal-decorators", {"legacy": true}],
        "@babel/plugin-proposal-class-properties",
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }] //配置antd
    ]
  }